--- a/Makefile.in
+++ b/Makefile.in
@@ -19,14 +19,14 @@
 
 runtest: build_table_apps test
 	@echo "running libsrtp test applications..."
-	crypto/test/cipher_driver$(EXE) -v >/dev/null
